{
  "name": "msgpack-nodejs",
  "version": "1.0.1",
  "description": "Node.js implementation of MessagePack encoder/decoder",
  "keywords": [
    "msgpack",
    "messagepack"
  ],
  "type": "module",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "esbuild": "0.16.10"
  },
  "devDependencies": {
    "husky": "^8.0.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "standard": "^17.0.0"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "test.html",
      "dist/"
    ]
  },
  "scripts": {
    "test": "mocha",
    "testcov": "nyc --reporter=text mocha",
    "lint": "standard",
    "build": "esbuild browser.js --bundle --target=chrome67,firefox68,safari14,edge79 --outfile=dist/index.js",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artyomliou/msgpack-nodejs.git"
  },
  "author": "Artyom Liou",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artyomliou/msgpack-nodejs/issues"
  },
  "homepage": "https://github.com/artyomliou/msgpack-nodejs#readme"
}
